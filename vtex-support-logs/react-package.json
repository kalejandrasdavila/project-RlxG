{
  "version": "0.0.0",
  "name": "storerlx",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "preinstall": "node vtex-no-workers-browser.js",
    "postinstall": "node vtex-no-workers-browser.js",
    "prebuild": "node vtex-no-workers-browser.js",
    "prestart": "node vtex-no-workers-browser.js",
    "predev": "node vtex-no-workers-browser.js",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "lint:locales": "intl-equalizer",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --watch --maxWorkers=1",
    "test:coverage": "jest --coverage --maxWorkers=1",
    "test:ci": "jest --ci --coverage --watchAll=false --maxWorkers=1",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:stats": "webpack --json > webpack-stats.json",
    "optimize": "npm run lint && npm run test:ci"
  },
  "resolutions": {
    "expect": "^27.5.1",
    "babel-jest": "^27.5.1",
    "pretty-format": "^27.5.1",
    "@octokit/rest": "^19.0.0"
  },
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "messages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "messages/*.json": [
      "yarn lint:locales"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/jquery": "^3.5.33",
    "@types/node": "^18.19.127",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/react-helmet-async": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vtex/danger": "^0.2.7",
    "@vtex/intl-equalizer": "^2.5.0",
    "@vtex/prettier-config": "^0.3.5",
    "@vtex/tsconfig": "^0.6.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-vtex": "^12.8.11",
    "eslint-config-vtex-react": "^6.8.3",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "expect": "^27.5.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.5",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-helmet-async": "^1.0.1",
    "swiper": "^11.2.10",
    "jquery": "^3.7.1",
    "path-browserify": "^1.0.1"
  },
  "optionalDependencies": {
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5"
  }
}
